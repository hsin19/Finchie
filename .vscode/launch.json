{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "statement-fetcher(python): Current Module",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": true,
            "module": "${fileBasenameNoExtension}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/services/statement-fetcher",
            "env": {
                "PYTHONPATH": "${fileDirname}"
            }
        }
    ]
}